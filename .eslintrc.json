{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "never"],
        "quotes": ["error", "single"],
        "import/extensions": ["error", "ignorePackages"],
        "no-console": ["error", { "allow": ["log"] }],
        "consistent-return": "off",
        "import/prefer-default-export": "off",
        "no-underscore-dangle": "off"
    }
}
